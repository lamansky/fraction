        "floats",
        "numbers",
        "math"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fr. John Lamansky",
            "homepage": "https://lamansky.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {"Lamansky\\Fraction\\": "src/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
